{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "ng build --prod"
        }
      }
    ]
  ,
  "outputDirectory": "dist/app",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
